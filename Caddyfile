{
	frankenphp
    auto_https disable
    http_port	80
	https_port	0
}

# The domain name of your server
localhost {
	# Set the webroot to the public/ directory
	root * public/
	# Enable compression (optional)
	encode zstd br gzip
	# Execute PHP files from the public/ directory and serve assets
	php_server {
		try_files {path} index.php
	}
}
